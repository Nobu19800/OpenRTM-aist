		Name="sa3d_includes"
		Value="&quot;$(sa3d_root)\include&quot;;&quot;$(sa3d_root)\utilities\include&quot;"
	/>
	<UserMacro
		Name="sa3d_libdir"
		Value="&quot;$(sa3d_root)\lib&quot;;&quot;$(sa3d_root)\utilities\lib&quot;"
	/>
	<UserMacro
		Name="sa3d_lib"
		Value="hd.lib hl.lib hdu.lib hlu.lib"
	/>
	<UserMacro
		Name="sa3d_libd"
		Value="hd.lib hl.lib hdud.lib hlud.lib"
	/>
</VisualStudioPropertySheet>

